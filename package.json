{
  "name": "react-selectable",
  "version": "1.3.3",
  "description": "A component for react that allows mouse selection of child items",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/react-selectable.min.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/unclecheese/react-selectable.git"
  },
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack  dist/react-selectable.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack -p  dist/react-selectable.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "watch": "npm run build:commonjs -- --watch",
    "example": "webapack --config webpack.config.example.js",
    "prepublish": "npm run build"
  },
  "homepage": "http://unclecheese.github.io/react-selectable",
  "keywords": [
    "selectable",
    "selection",
    "mouse",
    "drag",
    "react"
  ],
  "author": "Uncle Cheese",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unclecheese/react-selectable/issues"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "cross-env": "^5.0.5",
    "lodash.throttle": "^4.0.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "webpack": "^3.5.5"
  }
}
